
<script>
    (function (cookies) {
        var cookiesArr = cookies.split(';');
        for (var i = 0; i < cookiesArr.length; i++) {
            if (cookiesArr[i].indexOf('viewed_commodities') < 0) {
                continue;
            }
            var value = cookiesArr[i].substr(cookiesArr[i].indexOf('=') + 1).trim();
            alert(value);
            switch (value) {
                case 'clear':
                    alert('execution Clear');
                    renderStorage();
                    return;
                case 'reset':
                    alert('execution Reset');
                    ajaxGotViewed();
                    return;
                default:
                    alert('JS block forming can not be engaged');
             }
        }
    }(decodeURIComponent(document.cookie)));
</script>